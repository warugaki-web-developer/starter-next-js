{
  "name": "starter-next-js",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint --dir src",
    "lint:fix": "next lint --dir src --fix",
    "lint:stylelint": "stylelint ./src/**/*.{css,jsx}",
    "lintfix:stylelint": "stylelint --fix ./src/**/*.{css,jsx}",
    "format:check": "prettier --check --ignore-path .gitignore './**/*.{js,jsx,json,css}'",
    "format:fix": "prettier --write --ignore-path .gitignore './**/*.{js,jsx,json,css}'",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "dependencies": {
    "@emotion/react": "^11.6.0",
    "next": "12.0.4",
    "prop-types": "^15.7.2",
    "react": "17.0.2",
    "react-dom": "17.0.2"
  },
  "devDependencies": {
    "@babel/core": "^7.16.0",
    "@emotion/babel-plugin": "^11.3.0",
    "@storybook/addon-actions": "^6.3.12",
    "@storybook/addon-essentials": "^6.3.12",
    "@storybook/addon-links": "^6.3.12",
    "@storybook/react": "^6.3.12",
    "@stylelint/postcss-css-in-js": "^0.37.2",
    "babel-loader": "^8.2.3",
    "eslint": "7.32.0",
    "eslint-config-airbnb": "^19.0.0",
    "eslint-config-next": "12.0.4",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-unused-imports": "^1.1.5",
    "postcss-syntax": "^0.36.2",
    "prettier": "^2.4.1",
    "stylelint": "^14.1.0",
    "stylelint-config-recess-order": "^3.0.0",
    "stylelint-config-standard": "^24.0.0"
  }
